<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gradient_6"
    tools:context="creator.test.logintest.AreaEvaluacionMenuActivity">

    <Button
        android:id="@+id/btnIngresar2"
        android:layout_width="275dp"
        android:layout_height="40dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="136dp"
        android:background="@drawable/borde_redondo2"
        android:text="Ingresar Area de Evaluacion"
        tools:layout_editor_absoluteX="-6dp"
        tools:layout_editor_absoluteY="83dp" />

    <Button
        android:id="@+id/btnEliminar2"
        android:layout_width="275dp"
        android:layout_height="40dp"
        android:layout_alignStart="@+id/btnIngresar2"
        android:layout_below="@+id/btnIngresar2"
        android:layout_marginTop="71dp"
        android:background="@drawable/borde_redondo2"
        android:text="Eliminar Area de Evaluacion"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="161dp" />

</RelativeLayout>
