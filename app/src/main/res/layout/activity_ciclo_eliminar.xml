<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="creator.test.logintest.CicloEliminarActivity">

    <TextView
        android:id="@+id/textView17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="69dp"
        android:text="Numero de Ciclo"
        android:textColor="@android:color/background_dark" />

    <EditText
    android:id="@+id/editNumeroCiclo"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/textView17"
    android:layout_centerHorizontal="true"
    android:ems="10"
    android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/editNumeroCiclo"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="58dp"
        android:text="AÃ±o Ciclo"
        android:textColor="@android:color/background_dark" />

    <EditText
    android:id="@+id/editAnioCiclo"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_below="@+id/textView18"
    android:layout_centerHorizontal="true"
    android:ems="10"
    android:inputType="textPersonName" />

    <Button
    android:id="@+id/buttonCrear"
    android:layout_width="250dp"
    android:layout_height="30dp"
    android:layout_below="@+id/editAnioCiclo"
    android:layout_centerHorizontal="true"
    android:layout_marginTop="48dp"
    android:background="@drawable/borde_redondo"
    android:onClick="eliminarCiclo"
    android:text="Eliminar" />

</RelativeLayout>